---
exclude_paths:
  - .ansible/
  - .cache/
  - .deprecated/
  - .facts_cache/
  - .github/
  - .mypy_cache/
  - .pre-commit-config.yaml
  - .pre-commit/
  - .prettierrc.yaml
  - .pytest_cache/
  - .ruff_cache
  - .venv/
  - .vscode/
  - docker-compose.yml

skip_list:
  - name[play]
  - package-latest
  - yaml[indentation]
